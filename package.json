  },
  "homepage": "https://github.com/dnprocks/api-currency-register#readme",
  "dependencies": {
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
